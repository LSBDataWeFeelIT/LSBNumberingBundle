{
  "name": "lsb/numbering-bundle",
  "type": "symfony-bundle",
  "description": "Number generator for Symfony applications",
  "license": "MIT",
  "keywords": [
    "symfony",
    "number",
    "generator"
  ],
  "homepage": "https://lsb.pl",
  "authors": [
    {
      "name": "Piotr Bazylewicz",
      "email": "piotr.bazylewicz@lsb.com.pl"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": "5.0.*",
    "doctrine/orm": "2.*",
    "doctrine/doctrine-bundle": "2.*",
    "symfony/validator": "5.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "symfony/phpunit-bridge": "^5.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "LSB\\NumberingBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  }
}
